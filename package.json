{
  "name": "dialog777",
  "displayName": "Dialogo",
  "description": "a dialog system for game",
  "version": "2.0.1",
  "publisher": "SillyCat",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/wiltchamberian/Dialogo.git"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "themes": [
      {
          "label": "My Custom Theme",
          "uiTheme": "vs", 
          "path": "./myCustomTheme.json"
      }
    ],
    "languages": [{
      "id": "dialogo",
      "aliases": ["dialogo", "dialogo"],
      "extensions": [".dlg"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dialogo",
      "scopeName": "text.dlg",
      "path": "./syntaxes/dialogo.tmLanguage.json"
    }]
  }
}
